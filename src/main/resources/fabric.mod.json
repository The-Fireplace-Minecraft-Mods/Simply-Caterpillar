{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",

  "name": "${modDisplayName}",
  "description": "${modDescription}",
  "authors": [
    "${modAuthors}"
  ],
  "contact": {
    "homepage": "${modDisplayURL}",
    "sources": "${modDisplayURL}",
    "issues": "${modIssueTrackerURL}"
  },
  "license": "${modLicense}",
  "icon": "assets/${modId}/${modLogoFile}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "${modGroup}.Caterpillar"
    ],
    "client": [
      "${modGroup}.CaterpillarClient"
    ],
    "modmenu": [
      "${modGroup}.compat.modmenu.ModMenuIntegration"
    ]
  },
  "mixins": [
    "${modId}.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${fabricLoaderVersion}",
    "fabric-api": "*",
    "minecraft": ">=1.20.2-",
    "java": ">=${minecraftJavaVersion}"
  },

  "suggests": {
    "modmenu": "*",
    "cloth-config": "*"
  }
}
